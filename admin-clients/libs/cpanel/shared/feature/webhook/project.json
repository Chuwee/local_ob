{
  "name": "cpanel-shared-feature-webhook",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/cpanel/shared/feature/webhook/src",
  "prefix": "admin-clients",
  "projectType": "library",
  "tags": ["scope:cpanel", "type:feature"],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/cpanel/shared/feature/webhook/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/cpanel/shared/feature/webhook/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/cpanel/shared/feature/webhook/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/cpanel/shared/feature/webhook/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
