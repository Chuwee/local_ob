{
  "name": "@admin-clients/shared-common-ui-code-editor",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/common/ui/code-editor/src",
  "prefix": "ob",
  "projectType": "library",
  "tags": ["scope:shared", "type:ui"],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/shared/common/ui/code-editor/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/common/ui/code-editor/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/common/ui/code-editor/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/common/ui/code-editor/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
