{
  "name": "shared-common-ui-components",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/shared/common/ui/components/src",
  "prefix": "admin-clients",
  "tags": ["scope:shared", "type:ui"],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "inputs": ["styled", "^production"],
      "options": {
        "project": "libs/shared/common/ui/components/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/common/ui/components/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/common/ui/components/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/common/ui/components/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/shared/common/ui/components/.storybook",
        "browserTarget": "shared-common-ui-components:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/shared-ui-components",
        "configDir": "libs/shared/common/ui/components/.storybook",
        "browserTarget": "shared-common-ui-components:build-storybook",
        "compodoc": false,
        "styles": ["libs/shared/common/ui/components/src/styles.scss"]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "shared-common-ui-components:build-storybook",
        "staticFilePath": "dist/storybook/shared-ui-components"
      },
      "configurations": {
        "ci": {
          "buildTarget": "shared-common-ui-components:build-storybook:ci"
        }
      }
    }
  }
}
