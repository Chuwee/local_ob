{
  "name": "shi-panel-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/shi-panel-e2e/src",
  "projectType": "application",
  "tags": [],
  "implicitDependencies": ["shi-panel"],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/shi-panel-e2e/cypress.config.ts",
        "devServerTarget": "shi-panel:serve",
        "testingType": "e2e"
      },
      "outputs": ["{projectRoot}/mochawesome-report/**"],
      "configurations": {
        "production": {
          "devServerTarget": "shi-panel:serve:production"
        }
      }
    },
    "e2e-remote": {
      "executor": "@OneboxTM/tools:e2e",
      "options": {
        "upload": true,
        "reports": "apps/shi-panel-e2e/mochawesome-report",
        "environments": "apps/shi-panel-e2e/environments.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
