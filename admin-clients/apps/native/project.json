{
  "name": "native",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "admin-clients",
  "sourceRoot": "apps/native/src",
  "tags": ["scope:native", "type:app"],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "inputs": [
        "styled",
        "^production",
        "!{projectRoot}/android/**/*",
        "!{projectRoot}/android/.gradle/**/*",
        "!{projectRoot}/ios/**/*"
      ],
      "options": {
        "outputPath": "dist/apps/native",
        "index": "apps/native/src/index.html",
        "browser": "apps/native/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/native/tsconfig.app.json",
        "assets": [
          "apps/native/src/environment.js",
          "apps/native/src/assets",
          {
            "glob": "**/*.svg",
            "input": "node_modules/ionicons/dist/ionicons/svg",
            "output": "./svg"
          }
        ],
        "styles": ["apps/native/src/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["apps/native/src/assets/sass", ""]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "8kb"
            }
          ]
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "proxyConfig": "apps/native/proxy.conf.js",
          "buildTarget": "native:build:production"
        },
        "development": {
          "proxyConfig": "apps/native/proxy.conf.js",
          "buildTarget": "native:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "native:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "hasTypeAwareRules": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/native/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "native:build"
      }
    },
    "cap": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "--help"
      }
    },
    "add": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "add"
      },
      "configurations": {
        "ios": {
          "cmd": "add ios"
        },
        "android": {
          "cmd": "add android"
        }
      }
    },
    "copy": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "copy"
      },
      "configurations": {
        "ios": {
          "cmd": "copy ios"
        },
        "android": {
          "cmd": "copy android"
        }
      }
    },
    "open": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "open"
      },
      "configurations": {
        "ios": {
          "cmd": "open ios"
        },
        "android": {
          "cmd": "open android"
        }
      }
    },
    "run": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "run"
      },
      "configurations": {
        "ios": {
          "cmd": "run ios"
        },
        "android": {
          "cmd": "run android"
        }
      }
    },
    "sync": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "sync"
      },
      "configurations": {
        "ios": {
          "cmd": "sync ios"
        },
        "android": {
          "cmd": "sync android"
        }
      }
    },
    "update": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "update"
      },
      "configurations": {
        "ios": {
          "cmd": "update ios"
        },
        "android": {
          "cmd": "update android"
        }
      }
    },
    "deploy": {
      "executor": "@OneboxTM/tools:deploy",
      "options": {
        "buildOutputPath": "dist/apps/native/browser",
        "environments": "apps/native/environments.json",
        "removeMode": "DEPRECATE"
      }
    },
    "remove": {
      "executor": "@OneboxTM/tools:remove",
      "options": {
        "environments": "apps/native/environments.json"
      }
    },
    "bundle": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/native/android"
      },
      "dependsOn": ["android-copy"],
      "configurations": {
        "pre": {
          "command": "gradlew bundlePreRelease"
        },
        "pro": {
          "command": "gradlew bundleProRelease"
        }
      }
    },
    "apk": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/native/android"
      },
      "dependsOn": ["android-copy"],
      "configurations": {
        "pre": {
          "command": "gradlew assemblePreRelease"
        },
        "pro": {
          "command": "gradlew assembleProRelease"
        }
      }
    },
    "android-copy": {
      "executor": "nx:run-commands",
      "dependsOn": ["build", "environment"],
      "options": {
        "cwd": "apps/native",
        "parallel": false,
        "commands": [
          "npx cap copy android",
          "cp ./src/environment.js ./android/app/src/main/assets/public"
        ]
      }
    },
    "info": {
      "executor": "@OneboxTM/tools:info",
      "options": {
        "buildOutputPath": "dist/apps/native/browser"
      }
    },
    "environment": {
      "executor": "@OneboxTM/tools:env",
      "configurations": {
        "pre": {
          "env": "pre"
        },
        "pro": {
          "env": "pro"
        }
      }
    }
  }
}
