{
  "extends": ["plugin:playwright/recommended", "../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["e2e/**/*.{ts,js,tsx,jsx}"],
      "rules": {}
    },
    {
      "files": ["e2e/**/*.spec.ts"],
      "rules": {
        "@typescript-eslint/no-restricted-imports": [
          "error",
          {
            "paths": [{
              "name": "@playwright/test",
              "importNames": ["test"],
              "message": "Please use 'test' import from /fixtures/spBranchParamsToGoto"
            }]
          }
        ]
      }
    }
  ]
}
